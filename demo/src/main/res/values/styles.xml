<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="allStyles">
        <item name="textBold">true</item>
        <item name="textColor">#ff003b</item>
        <item name="textFont">fonts/dosis.otf</item>
        <item name="textSize">16sp</item>
        <item name="colorBackground">#d4d0d0</item>
        <item name="solidBackground">true</item>
        <item name="strokeWidth">2dp</item>
        <item name="strokeColor">#bcb9b9</item>
        <item name="iconLeft">@drawable/ic_cloud_upload_black_24dp</item>
        <item name="length">LONG</item>
        <item name="cornerRadius">14dp</item>
    </style>

    <style name="style2">
        <item name="textColor">#ffffff</item>
        <item name="colorBackground">#8193b4</item>
    </style>

    <style name="empty">
    </style>

</resources>
